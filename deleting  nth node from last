
class Solution {
public:
    ListNode* removeNthFromEnd(ListNode* head, int n) {
        int cnt=0;
       ListNode*temp=head;
       while(temp!=nullptr){
        cnt++;
        temp=temp->next;
       }
       if(cnt<n){
        return head;
        
       }
       if(cnt==n){
        ListNode*newhead=head->next;
        ListNode*del=head;
        delete(del);
        return newhead;
       }
       temp=head;
       int count=1;
       while(count!=cnt-n){
        temp=temp->next;
        count++;
       }
       ListNode*res=temp->next;
       temp->next=res->next;
       delete(res);
    

       return head;
        
    }
};
